define("echarts/util/shape/halfSmoothPolygon",["require","zrender/shape","zrender/shape/base"],function(require){function e(){this.type="halfSmoothPolygon"}return e.prototype={buildPath:function(e,t){var i=t.pointList;if(!(i.length<2))if(t.smooth){var r=this.smoothBezier(i.slice(0,-2),t.smooth);e.moveTo(i[0][0],i[0][1]);for(var n,a,o,s=i.length,l=0;s-3>l;l++)n=r[2*l],a=r[2*l+1],o=i[l+1],e.bezierCurveTo(n[0],n[1],a[0],a[1],o[0],o[1]);e.lineTo(i[s-2][0],i[s-2][1]),e.lineTo(i[s-1][0],i[s-1][1]),e.lineTo(i[0][0],i[0][1])}else require("zrender/shape").get("polygon").buildPath(e,t)}},require("zrender/shape/base").derive(e),require("zrender/shape").define("halfSmoothPolygon",new e),e});